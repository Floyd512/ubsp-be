<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.udan.ubsp.integration.mapper.SyncTaskExecutionMapper">

    <select id="selectByJobId" resultType="com.udan.ubsp.integration.entity.SyncTaskExecutionEntity">
        SELECT *
        FROM ubsp_di_sync_task_execution
        WHERE sea_tunnel_job_id = #{jobId}
        AND is_deleted = 0
    </select>

</mapper>